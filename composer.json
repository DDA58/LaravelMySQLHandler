{
    "name": "dda58/mysqlhandlerforlaravelquerybuilder",
    "description": "Allow use MySQL handlers for Laravel query builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitrii Denisov",
            "email": "dda58denisov@gmail.com",
            "homepage": "https://github.com/DDA58"
        }
    ],
    "homepage": "https://github.com/dda58/mysqlhandlerforlaravelquerybuilder",
    "keywords": ["Laravel", "MySQLHandlerForLaravelQueryBuilder"],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": ">=5.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0",
        "orchestra/testbench": ">=3.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "DDA58\\MySQLHandlerForLaravelQueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DDA58\\MySQLHandlerForLaravelQueryBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DDA58\\MySQLHandlerForLaravelQueryBuilder\\HandlerServiceProvider"
            ],
            "aliases": {
                "HandlerManager": "DDA58\\MySQLHandlerForLaravelQueryBuilder\\Facades\\HandlerManager",
                "HandlerContainer": "HandlerContainer"
            }
        }
    }
}
